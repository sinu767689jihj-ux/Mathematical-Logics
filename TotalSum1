
import java.util.Scanner;
class TotalSum1
{
    void totalSum(int number)
    {
        int sum=0;
        while(number!=0)
        {
           int r=number%10; 
           sum=sum+r;
           number/=10;
        }
        System.out.println("Total Sum:"+sum);
    }
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int number=sc.nextInt();
        TotalSum1 s=new TotalSum1();
        s.totalSum(number);
        sc.close();
    }
}
