public class SumOddNumbers {
    void SumOddNumber(int n)
    {
        int sum=0;
        for(int i=1;i<n;i++)
        {
            if(i%2==1)
            {
               sum+=i; 
            }
        }
        System.out.println("Sum of odd numbers is: "+sum);
    }
    public static void main(String[] args)
    {
        SumOddNumbers obj=new SumOddNumbers();
        obj.SumOddNumber(10);
    }
}
