public class FibonacciSeriesUsingMethod {
    void fibonacciSeries(int a,int b)
    {
        a=0;
        b=1;
        for(int i=2;i<8;i++)
        {
            int c=a+b;
            System.out.print(" "+c);
            a=b;
            b=c;
        }
    }
    public static void main(String[] args)
    {
        FibonacciSeriesUsingMethod obj=new FibonacciSeriesUsingMethod();
        obj.fibonacciSeries(0,1);
    }
}
