import java.util.Scanner;
public class Exponenial
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number:");
        int n=sc.nextInt();
        int i;
        for(i=1;i<=n;i++)
        {
            System.out.print("x"+"^"+i);
            if(i<n)
            {
                System.out.print(" + ");
            }
        }
        sc.close();
    }
}
