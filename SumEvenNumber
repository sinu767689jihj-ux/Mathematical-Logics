public class SumEvenNumber {
    void SumEvenNumber(int n)
    {
        int sum=0;
        for(int i=1;i<n;i++)
        {
            if(i%2==0)
            {
               sum+=i; 
            }
        }
        System.out.println("Sum of even numbers is: "+sum);
    }
    public static void main(String[] args)
    {
        SumEvenNumber obj=new SumEvenNumber();
        obj.SumEvenNumber(10);
    }
}
